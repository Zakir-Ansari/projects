{
  "version": 2,
  "builds": [
    {
      "src": "/app/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["./dist/**", "./node_modules/swagger-ui-dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/app.js"
    }
  ],
  "build": {
    "ignoreCommand": "git diff --quiet HEAD^ HEAD -- pick-kart/backend || exit 1"
  }
}
